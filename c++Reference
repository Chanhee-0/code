#include<iostream>
#include<string>
#include<list>
#include<vector>
#include<array>
#include<queue>
#include<set>
#include<map>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	string s; // string 선언

	s = "hello"; // string 초기화

	string s("hello"); // string 초기화

	s.assign("hello"); // string 초기화

	s.clear(); // string 전체를 지운다

	s.size(); // string의 사이즈 반환


	list<int> l;

	

	vector<int> v;



	array <int, 50> arr;



	priority_queue<int> q; // priority_queue 선언 내림차순

	priority_queue<int, vector<int>, greater<int> > pq; // priority_queue 선언 오름차순


	return 0;
}
